{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "plugin:react/jsx-runtime"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 13,
        
        "sourceType": "module"
    },
    "plugins": [
        "react", "prettier"
    ],

    "prettier/prettier": [
  "error",
  {
    "endOfLine": "auto"
  }
],
    "rules": {
        "prettier/prettier": "error",
        "react/react-in-jsx-scope": "off",
        "react/prop-types": "off"
    }
}
